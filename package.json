{
  "name": "fed",
  "version": "1.0.0",
  "description": "前端导航",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "stylus -u autoprefixer-stylus styl/index.styl -o css/ -c",
    "build": "npm run build:css && npm run min && ssr",
    "watch": "npm run watch:css && npm run watch:js",
    "watch:css": "onchange 'styl/**/*.styl' -v -- npm run build",
    "watch:js": "onchange 'js/index.js' './data.json' -v -- npm run min",
    "min": "npm run min:js && npm run min:data",
    "min:js": "uglifyjs js/index.js -o js/index.min.js",
    "min:data": "node js/build.json.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.5",
    "autoprefixer-stylus": "^0.9.2",
    "onchange": "^2.2.0",
    "postcss-cli": "^2.5.1",
    "stylus": "^0.54.2",
    "uglify-js": "^2.6.2"
  }
}
